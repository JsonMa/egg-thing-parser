{
  "name": "egg-thing-parser",
  "version": "1.2.30",
  "description": "thing model parser",
  "eggPlugin": {
    "name": "thingParser"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "thing",
    "thing model",
    "thing parser",
    "thing-parser"
  ],
  "dependencies": {
    "ajv": "^6.10.2",
    "egg-ajv": "^1.0.1",
    "lodash": "^4.17.15",
    "thing-assert": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "chance": "^1.0.18",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.23.1",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "husky": "^3.0.3",
    "validate-branch-name": "^1.0.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod",
    "branchname": "validate-branch-name"
  },
  "files": [
    "app.js",
    "agent.js",
    "config",
    "app",
    "lib"
  ],
  "ci": {
    "version": "8, 10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "validate-branch-name"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonma/egg-thing-parser.git"
  },
  "bugs": {
    "url": "https://github.com/jsonma/egg-thing-parser/issues"
  },
  "homepage": "https://github.com/jsonma/egg-thing-parser#readme",
  "author": "jsonma ",
  "license": "MIT"
}
